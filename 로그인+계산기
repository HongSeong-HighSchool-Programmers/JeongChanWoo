Public Class Form6

    Private num3 As Integer
    Private num4 As Integer
    Private n As Integer

    Private Sub Button1_Click_6(sender As Object, e As EventArgs) Handles Button6.Click
        num1 = TextBox4.Text
        num2 = TextBox5.Text

        If CheckBox5.Checked Then
            m = num3 + num4
            TextBox6.Text = m

        ElseIf CheckBox6.Checked Then
            m = num3 - num4
            TextBox6.Text = m

        ElseIf CheckBox7.Checked Then
            m = num3 * num4
            TextBox6.Text = m

        ElseIf CheckBox8.Checked Then
            n = num3 / num4
            TextBox6.Text = m

        End If
    End Sub

